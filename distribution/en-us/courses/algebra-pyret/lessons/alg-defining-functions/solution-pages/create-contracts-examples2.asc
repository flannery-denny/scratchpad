[.pyret]
= Creating Contracts From Examples

%CURRICULUMspan% class="web-page-only"%BEGINCURRICULUMspan%Referenced from lesson link:{pathwayrootdir}lessons/alg-defining-functions/index.shtml[Defining Functions]

%ENDCURRICULUMspan%




*Directions*: For each of the following collections of Examples, write the contract
           for the function above the examples.



[.plain.exercises]

1. %CURRICULUMdiv% class="ExerciseListItem"%BEGINCURRICULUMdiv%%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%third-root :{zwsp}: Number -> Number%ENDCURRICULUMtt%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%examples:
  third-root(16) is expt(16, 1/3)
  third-root(125) is expt(125, 1/3)
  third-root(625) is expt(625, 1/3)
end%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%

2. %CURRICULUMdiv% class="ExerciseListItem"%BEGINCURRICULUMdiv%%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%sentence-length :{zwsp}: String, Number -> Number%ENDCURRICULUMtt%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%examples:
  sentence-length("hello", 5) is string-length("hello") + 5
  sentence-length("please", 6) is string-length("please") + 6
  sentence-length("goodbye", 10) is string-length("goodbye") + 10
end%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%

3. %CURRICULUMdiv% class="ExerciseListItem"%BEGINCURRICULUMdiv%%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%upside-down :{zwsp}: Image -> Image%ENDCURRICULUMtt%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%examples:
  upside-down(triangle(50, "solid", "yellow")) is
    rotate(180, triangle(50, "solid", "yellow"))
  upside-down(star(150, "solid", "blue")) is
    rotate(180, star(150, "solid", "blue"))
end%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%

4. %CURRICULUMdiv% class="ExerciseListItem"%BEGINCURRICULUMdiv%%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%product-squared :{zwsp}: Number, Number -> Number%ENDCURRICULUMtt%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%examples:
  product-squared(5, 6) is sqr(5 *  6)
  product-squared(10, 17) is sqr(10 *  17)
  product-squared(2, 5) is sqr(2 *  5)
end%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%

5. %CURRICULUMdiv% class="ExerciseListItem"%BEGINCURRICULUMdiv%%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%half-image :{zwsp}: String -> Image%ENDCURRICULUMtt%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%examples:
  half-image("www.images.com/cat_image") is
    scale(0.5, bitmap/url("www.images.com/cat_image"))
  half-image("www.images.com/dog_image") is
    scale(0.5, bitmap/url("www.images.com/dog_image"))
end%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%

6. %CURRICULUMdiv% class="ExerciseListItem"%BEGINCURRICULUMdiv%%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%triple :{zwsp}: Number -> Number%ENDCURRICULUMtt%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="obeyspaces"%BEGINCURRICULUMdiv%%CURRICULUMtt% class="pyret"%BEGINCURRICULUMtt%examples:
  triple(4) is 4 * 3
  triple(10) is 10 * 3
  triple(3) is 3 * 3
end%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%




